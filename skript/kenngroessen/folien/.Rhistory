d <- tibble(
X=0:4,
Y=c(1.2, 3.2, 0.5, 0.9, 1.1),
Z=c("A", "B", "C", "D", "E")
)
d
str(d)
kable(d)
d$X
d$Z
d$Z[4]
xmid <- mean(d$X)
ymid <- mean(d$Y)
beta <- sum((d$X - xmid) * (d$Y - ymid)) / sum((d$X - xmid)^2)
alpha <- ymid - beta * xmid
R2 <- sum((alpha + beta * d$X - ymid)^2) / sum((d$Y - ymid)^2)
paste0("alpha = ", alpha, ", beta = ", beta, ", R2 = ", signif(R2, 5))
m <- lm(Y ~ X, data = d)
summary(m)
